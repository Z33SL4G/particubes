												</video>
											</div>
											<div class="clear"></div>
										</div>
									{{ else if .Code }}
										<pre>{{ .Code }}</pre>
									{{ end }}
								{{ end }}
							</div>
						</div>
					{{ end }}
				{{ end }}

				{{ if or .Functions .BaseFunctions}} 
